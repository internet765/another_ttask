  "devDependencies": {
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.8.7",
    "stylelint": "^15.5.0",
    "stylelint-config-standard": "^33.0.0"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "animate.min.css": "0.0.3",
