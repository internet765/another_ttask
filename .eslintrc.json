{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "prefer-const": "error",
        "quotes": ["warn", "single"],
        "indent": ["warn", 2],
        "semi": ["warn", "always"]
    }
}
